<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<HBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <TreeView fx:id="planView" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS" />
      <BorderPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
         <top>
            <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
               <children>
                  <VBox fx:id="buttonBox" alignment="TOP_CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="150.0" spacing="5.0">
                     <children>
                        <Button mnemonicParsing="false" onAction="#onImportAllAction" prefHeight="25.0" prefWidth="76.0" text="Import All" />
                        <Button mnemonicParsing="false" onAction="#onImportSelectedAction" prefHeight="25.0" prefWidth="139.0" text="Import Selected" />
                        <Button mnemonicParsing="false" onAction="#onRefreshAction" text="Refresh" />
                     </children>
                     <padding>
                        <Insets top="10.0" />
                     </padding>
                  </VBox>
               </children>
            </VBox>
         </top>
         <bottom>
            <VBox fx:id="progressBox" BorderPane.alignment="CENTER">
               <children>
                  <ProgressBar fx:id="totalProgress" prefWidth="200.0" progress="0.0" />
                  <ProgressBar fx:id="itemProgress" prefWidth="200.0" progress="0.0" />
               </children>
            </VBox>
         </bottom>
      </BorderPane>
   </children>
</HBox>
