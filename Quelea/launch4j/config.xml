
<launch4jConfig>
    <headerType>gui</headerType>
    <jar>..\dist\Quelea.jar</jar>
    <jre>
        <path>winjre</path>
    </jre>
    <outfile>..\dist\quelea.exe</outfile>
    <chdir>.</chdir>
    <errTitle>Error</errTitle>
    <priority>normal</priority>
    <downloadUrl>http://www.oracle.com/technetwork/java/javase/downloads/index.html</downloadUrl>
    <supportUrl>https://groups.google.com/group/quelea-discuss</supportUrl>
    <customProcName>false</customProcName>
    <stayAlive>false</stayAlive>
    <manifest></manifest>
    <icon>../icons/logo.ico</icon>
    <messages>
        <startupErr>Sorry, there was an error starting the program.</startupErr>
        <bundledJreErr>Sorry, there was an error with the bundled JRE.</bundledJreErr>
        <jreVersionErr>You're running a version of Java that's too old - you need at least version 7.</jreVersionErr>
        <launcherErr>Sorry, there was an error launching Quelea.</launcherErr>
    </messages>
    <versionInfo>
        <fileVersion>0.6.0.0</fileVersion> <!-- Must be format x.x.x.x otherwise launch4j complains -->
        <productVersion>0.6.0.0</productVersion> <!-- Must be format x.x.x.x otherwise launch4j complains -->
        <txtProductVersion>0.6</txtProductVersion>
        <txtFileVersion>0.6</txtFileVersion>
        <productName>Quelea</productName>
        <copyright>GPLv3</copyright>
        <originalFilename>Quelea.exe</originalFilename>
        <companyName>Michael Berry</companyName>
        <fileDescription>Quelea executable</fileDescription>
        <internalName>Quelea</internalName>
    </versionInfo>
</launch4jConfig>
